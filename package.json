{
  "name": "querty",
  "version": "0.2.3",
  "description": "querty - a query-based http client",
  "repository": {
    "url": "https://github.com/joe-crick/querty"
  },
  "jsnext:main": "dist/index.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
